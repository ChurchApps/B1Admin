{
    "extends": "react-app",
    "parser":"@typescript-eslint/parser",
    "parserOptions":{
      "ecmaFeatures":{
        "jsx":true
      },
      "ecmaVersion":2018,
      "sourceType":"module"
    },
    "plugins":[
      "react",
      "jsx-a11y",
      "@typescript-eslint"
    ],
    "rules":{
      "react-hooks/exhaustive-deps":"error",
      "no-var":"error",
      "brace-style":"error",
      "prefer-template":"error",
      "space-before-blocks":"error",
      "import/prefer-default-export":"off"
    }
}